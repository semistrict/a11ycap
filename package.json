{
  "name": "pwsnapshot-workspace",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm --filter libpwsnapshot build && (cd testpage && pnpm run build) && playwright test",
    "test:headed": "pnpm --filter libpwsnapshot build && playwright test --headed",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0"
  }
}