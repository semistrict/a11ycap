{
  "name": "a11ycap",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-focused accessibility snapshot functionality extracted from Playwright",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run clean && pnpm run typecheck && vite build && tsc -p tsconfig.declarations.json",
    "typecheck": "tsc --noEmit",
    "test": "pnpm run build && playwright test",
    "test:headed": "pnpm run build && playwright test --headed",
    "test:server": "node test-server.js",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "clean": "rm -rf dist",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "accessibility",
    "aria",
    "snapshot",
    "ai",
    "browser",
    "playwright"
  ],
  "author": "Ramon Nogueira <https://github.com/semistrict>",
  "license": "Apache-2.0",
  "dependencies": {
    "yaml": "2.6.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@playwright/test": "^1.48.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/semistrict/a11ycap.git",
    "directory": "a11ycap"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}